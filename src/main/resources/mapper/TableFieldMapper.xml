<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.bucheng.dao.TableFieldMapper">
   <select id="countName" resultType="int" parameterType="cn.bucheng.model.vo.TableFieldVO">
       select count(*) from t_table_field where name in
       <foreach collection="vos" item="item" index="index" separator="," open="(" close=")">
           #{item.fieldName}
       </foreach>
   </select>

</mapper>